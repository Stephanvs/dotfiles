    "build": {
      "model": "anthropic/claude-opus-4-5"
    },
    "plan": {
      "model": "anthropic/claude-opus-4-5",
      "tools": {
        "write": false,
        "edit": false,
        "bash": false
      }
    },
    "docs": {
      "prompt": "{file:./prompts/documentation.txt}",
      "tools": {
        "write": true,
        "edit": true,
        "bash": false,
        "read": true,
        "grep": true,
        "glob": true
      }
    },
    "review": {
      "prompt": "{file:./prompts/code_review.txt}",
      "tools": {
        "write": false,
        "edit": false,
        "bash": false,
        "read": true,
        "glob": true,
        "grep": true,
        "list": true,
        "webfetch": true
      }
    }
