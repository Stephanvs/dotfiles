{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "base": "#1e1e2e",
    "blue": "#89B4FA",
    "closer": "p:prompt",
    "dotnet": "p:node",
    "error": "p:red",
    "executiontime": "p:maroon",
    "flamingo": "#f2cdcd",
    "folder": "p:path",
    "git-ahead": "#f2cdcd",
    "git-behind": "#f2cdcd",
    "git-diverged": "#f2cdcd",
    "git-foreground": "#a6e3a1",
    "git-modified": "#cba6f7",
    "green": "#a6e3a1",
    "lavender": "#B4BEFE",
    "maroon": "#eba0ac",
    "mauve": "#cba6f7",
    "node": "#74c7ec",
    "os": "p:blue",
    "path": "#94e2d5",
    "peach": "#fab387",
    "pink": "#f5c2e7",
    "prompt": "p:green",
    "red": "#f38ba8",
    "root": "#ffff66",
    "rosewater": "#f5e0dc",
    "sapphire": "#74c7ec",
    "sky": "#89dceb",
    "teal": "#94e2d5",
    "text": "#cdd6f4",
    "yellow": "#f9e2af"
  },
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "{{ if .Error }}[\uebfb]: {{ .Code }}{{ else }}[\u2192]{{ end }}",
          "foreground": "p:prompt",
          "type": "status",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:error{{ end }}"
          ]
        },
        {
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "cache_duration": "none",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "style": "diamond",
          "foreground": "p:os",
          "type": "os"
        },
        {
          "style": "diamond",
          "template": " {{ if .SSHSession }}\udb82\udcc0 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "foreground": "p:blue",
          "type": "session"
        },
        {
          "style": "diamond",
          "foreground": "p:blue",
          "type": "root"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "<p:folder> \ue5fe </>",
            "style": "agnoster_full",
            "mapped_locations": {
              "~/code": "",
              "~/Code": ""
            }
          },
          "style": "diamond",
          "foreground": "p:path",
          "type": "path"
        },
        {
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "foreground": "p:git-foreground",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-modified{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ]
        },
        {
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "  {{ .Full }} ",
          "foreground": "p:node",
          "type": "bun"
        },
        {
          "style": "diamond",
          "foreground": "p:dotnet",
          "type": "dotnet"
        },
        {
          "style": "powerline",
          "template": "\ue7a8 {{ .Full }}",
          "foreground": "p:red",
          "type": "rust"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 500
          },
          "style": "plain",
          "foreground": "p:executiontime",
          "type": "executiontime"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": " <#f5c2e7>\udb81\ude27</>",
          "foreground": "p:closer",
          "type": "text"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
